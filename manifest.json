{
	"manifest_version": 3,
	"name": "IHateYoutubeAD",
	"version": "1.0.0",
	"description": "Escape the clutter of YouTube ads! (Author: SimSool)",
	"permissions": ["activeTab"],
	"icons": {
		"16": "icon.png",
		"48": "icon.png",
		"128": "icon.png",
	},
	"content_scripts": [{
		"matches": [
			"*://www.youtube.com/*",
			"*://m.youtube.com/*"
			"*://youtube.com/*",
		],
		"js": ["content.js"]
	}]
}